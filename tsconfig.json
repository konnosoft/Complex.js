{
  "compilerOptions": {
    "esModuleInterop": true
  },
  "typedocOptions": {
    "entryPointStrategy": "expand",
    "sort": ["source-order"],
    "katex": {
      "options": {
        "delimiters": [
          {
            "left": "$$",
            "right": "$$",
            "display": true
          },
          {
            "left": "$",
            "right": "$",
            "display": false
          }
        ]
      }
    }
  }
}
